{
  "kind": "collectionType",
  "collectionName": "reservations",
  "info": {
    "singularName": "reservation",
    "pluralName": "reservations",
    "displayName": "Reservation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "reservation_id": {
      "type": "uid"
    },
    "date": {
      "type": "date"
    },
    "time_slot": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "cancelled"
      ],
      "default": "pending"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "coworking_space": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::coworking-space.coworking-space"
    }
  }
}