{
  "kind": "collectionType",
  "collectionName": "equipments",
  "info": {
    "singularName": "equipment",
    "pluralName": "equipments",
    "displayName": "Equipment",
    "description": "Equipments available in coworking spaces"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "spaces": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::space.space",
      "mappedBy": "equipments"
    },
    "price": {
      "type": "decimal"
    },
    "price_type": {
      "type": "enumeration",
      "enum": [
        "hourly",
        "daily",
        "one-time"
      ],
      "default": "one-time"
    },
    "bookings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::booking.booking",
      "mappedBy": "equipments"
    },
    "quantity": {
      "type": "integer",
      "default": 1
    }
  }
}