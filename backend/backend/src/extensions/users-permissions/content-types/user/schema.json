{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "required": true,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phone": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "fullname": {
      "type": "string"
    },
    "user_type": {
      "type": "enumeration",
      "enum": ["admin", "trainer", "student", "professional", "association"]
    },
    "etudiant_profil": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::etudiant-profil.etudiant-profil",
      "mappedBy": "user"
    },
    "association_profil": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::association-profil.association-profil",
      "mappedBy": "user"
    },
    "professionnel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::professionnel.professionnel",
      "mappedBy": "user"
    },
    "formateur_profil": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::formateur-profil.formateur-profil",
      "mappedBy": "user"
    },
    "bio": {
      "type": "text"
    },
    "city": {
      "type": "string"
    },
    "social_links": {
      "type": "json",
      "default": {
        "linkedin": "",
        "twitter": "",
        "instagram": ""
      }
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "emailPreferences": {
      "type": "json",
      "default": {
        "newsletter": true,
        "reservations": true,
        "payments": true,
        "courses": true,
        "marketing": false
      }
    },
    "subscription": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subscription.subscription",
      "mappedBy": "user"
    },
    "fcmToken": {
      "type": "string"
    },
    "otp_code": {
      "type": "string",
      "configurable": false,
      "private": true
    }
  }
}
